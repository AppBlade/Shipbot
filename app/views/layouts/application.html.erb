  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

  <div class="navbar navbar-inverse navbar-fixed-top">
    <div class="navbar-inner">
      <div class="container-fluid">
        <a class="brand" href="#">Title</a>
        <ul class="nav">
          <li class="active"><a href="#">Home</a></li>
          <li><a href="#">Link</a></li>
          <li><a href="#">Link</a></li>
        </ul>
      </div>
    </div>
  </div>

  <div class="container-fluid">
    <div class="row-fluid">
      <div class="span2">
        Span
      </div>
      <div class="span10">
        <% if content_for? :title %>
          <h2><%= yield :title %></h2>
        <% end %>
        <%= yield %>
      </div>
    </div>
  </div>

  <footer class="footer">
    <div class="container-fluid">
      Footer.
    </div>
  </footer>

  <%= render 'shared/go_squared' %>

</body>
</html>
