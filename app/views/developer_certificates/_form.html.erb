<%= simple_form_for(@developer_certificate, :multipart => true) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :keychain_export, :as => :file %>
    <%= f.input :keychain_export_passcode %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
